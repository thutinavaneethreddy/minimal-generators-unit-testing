{
  "version": 2,
  "defaultProject": "app1",
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "packageManager": "npm"
  },
  "generators": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "projects": {
    "app1": "apps/app1",
    "app1-e2e": "apps/app1-e2e",
    "app2": "apps/app2",
    "app2-e2e": "apps/app2-e2e",
    "lib1": "libs/lib1",
    "lib2": "libs/lib2",
    "tools": "tools"
  }
}
